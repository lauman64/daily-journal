<%- include("partials/header"); %>
<h1>Compose</h1>
<div class="content">
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label for="title">Title</label>
      <input class="form-control" type="text" name="postTitle" required/>
    </div>
    <div class="form-group">
      <label for="composeMessage">Post</label>
      <textarea class="form-control biggerText" name="postMessage" rows="8" cols="80" required></textarea>
    </div>
    <button class="btn btn-primary" type="submit">Publish</button>
  </form>
</div>
<%- include("partials/footer"); %>
